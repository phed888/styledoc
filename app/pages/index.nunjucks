<!-- index.nunjucks -->

{% extends "layout.nunjucks" %}

{% block content %}
    <h1>This is the index page</h1>
    <div class="gallery">
        {% for image in images %}
            <img src="{{ image.src }}" alt="{{ image.alt }}">
        {% endfor %}
    </div>
    {% import 'macros/nav-macro.nunjucks' as nav %}
    {{ nav.active('home') }}
{% endblock %}
